{
    "name": "sakai-ng",
    "version": "20.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --no-watch --live-reload=false",
        "start:dev": "ng serve --configuration development --no-watch --live-reload=false",
        "start:watch": "ng serve --configuration development --poll=5000",
        "start:prod": "ng serve --configuration production",
        "build": "ng build",
        "build:dev": "ng build --configuration development",
        "build:prod": "ng build --configuration production",
        "watch": "ng build --watch --configuration development",
        "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^20.3.6",
        "@angular/common": "^20.3.6",
        "@angular/compiler": "^20.3.6",
        "@angular/core": "^20.3.6",
        "@angular/forms": "^20.3.6",
        "@angular/platform-browser": "^20.3.6",
        "@angular/platform-browser-dynamic": "^20.3.6",
        "@angular/router": "^20.3.6",
        "@primeuix/themes": "^1.2.5",
        "@tailwindcss/postcss": "^4.1.15",
        "aos": "^2.3.4",
        "chart.js": "^4.5.1",
        "gsap": "^3.13.0",
        "lottie-web": "^5.13.0",
        "ng-lottie": "^0.3.2",
        "primeclt": "^0.1.5",
        "primeicons": "^7.0.0",
        "primeng": "^20.2.0",
        "rxjs": "~7.8.2",
        "tailwindcss-primeui": "^0.6.1",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
